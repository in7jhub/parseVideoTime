String[][] time = { 
{"4:56:14","4:58:03"},
{"4:59:28" ,"5:00:25"},
{"5:02:28" , "5:04:11"},
{"5:07:07" , "5:08:05"},
{"5:11:09" , "5:12:22"},
{"5:14:16" , "5:15:15"},
{"5:18:07" , "5:19:20"},
{"5:19:20" , "5:40:19"},
{"5:40:19" , "5:51:24"},
{"5:51:24" , "6:10:17"},
{"6:10:17" , "6:13:00"}};

for(int i = 0; i < time.length; i++){
  String[] a = time[i][0].split(":");
  String[] b = time[i][1].split(":");
  int e = 0;
  int f = 0;
  for(int j = 0; j < 3; j++){
    int c = Integer.parseInt(b[j]);
    int d = Integer.parseInt(a[j]);
    if(j == 0){
      e += (c*60000); 
      f += (d*60000);
    } else if (j == 1){
      e += (c*1000);
      f += (d*1000);
    } else if (j == 2){
      e += (c*10);
      f += (d*10);
    }
  }
  println(i+1+" : "+f+","+e);
}
